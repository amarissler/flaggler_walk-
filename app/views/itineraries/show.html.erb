<!--Social Networking Share buttons -->
<div class="container social">
  <div class="row">
    <div class="col-sm-4"></div>
    <div class="col-sm-4">
    Share your trai!
      <i class="fa fa-facebook-official"></i>
      <i class="fa fa-twitter-square"></i>
    </div>
    <div class="col-sm-4"></div>
  </div>
</div>

<!-- Map of itinerary -->
<div class="row map">
  <div id="map"></div>
</div>

<p id="trail-length"></p>


<!--image thumb nails to open as model -->
<div class="container-fluid">
  <%= render @itinerary.photos %>


<% content_tag(:p, @itinerary.id, id: "trail" ) %>
<% content_tag(:p, @itinerary.travel, id: "travel") %>
<% link_to 'Back', itineraries_path %>

</div>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAIAW3z8RyJAAq5KFcziTTMSk9fh42xyRc&callback=initialize"></script>
